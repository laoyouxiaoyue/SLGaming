          },
          {
            "name": "status",
            "in": "query",
            "type": "integer",
            "format": "int32",
            "description": "状态筛选：1=CREATED,2=PAID,3=ACCEPTED,4=IN_SERVICE,5=COMPLETED,6=CANCELLED,7=RATED"
